    <sys_variable_value action="INSERT_OR_UPDATE">
        <document>wf_activity</document>
        <document_key>caf40c1d0f186a4042f54e9ce1050efa</document_key>
        <order>1</order>
        <sys_class_name>sys_variable_value</sys_class_name>
        <sys_created_by>admin</sys_created_by>
        <sys_created_on>2016-06-27 17:41:22</sys_created_on>
        <sys_id>4ef40c1d0f186a4042f54e9ce1050efa</sys_id>
        <sys_mod_count>0</sys_mod_count>
        <sys_updated_by>admin</sys_updated_by>
        <sys_updated_on>2016-06-27 17:41:22</sys_updated_on>
        <value>if(current.variables.return_workflow=="No"){&#13;
		//Not a return workflow so a provisioning workflow&#13;
		workflow.scratchpad.addgroup=current.cat_item.model.u_install_group;&#13;
		//workflow.scratchpad.removegroup=current.cat_item.model.u_uninstall_group;&#13;
		//return 'yes';&#13;
}&#13;
else{&#13;
		//A return workflow so not a provisioning workflow&#13;
		workflow.scratchpad.addgroup=current.cat_item.model.u_uninstall_group;&#13;
		//workflow.scratchpad.removegroup=current.cat_item.model.u_install_group;&#13;
		//return 'no';&#13;
}&#13;
&#13;
var oGroup=new GlideRecord('sys_group');&#13;
if(oGroup.get(workflow.scratchpad.addgroup)){&#13;
	var oUGRecord=new GlideRecord('sys_user_grmembership');&#13;
	oUGRecord.initialize();&#13;
	oUGRecord.user=current.opened_by.sys_id;&#13;
	oUGRecord.group=oGroup.sys_id;&#13;
	oUGRecord.insert();&#13;
}&#13;
else{&#13;
	var inc=new GlideRecord('incident');&#13;
	inc.initialize();&#13;
	inc.short_description="Unable to add user to add group for software request"+current.number;&#13;
inc.update();&#13;
}</value>
