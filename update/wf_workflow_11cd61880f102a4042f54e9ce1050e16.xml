        <value>gs.addInfoMessage(current.user);&#13;
workflow.scratchpad.userID=current.user;&#13;
workflow.scratchpad.userName=current.user.user_name;&#13;
workflow.scratchpad.groupList=findAllAppGroups(workflow.scratchpad.userID);&#13;
gs.addInfoMessage("No. of groups with App_ pattern is:"+workflow.scratchpad.groupList.length);&#13;
workflow.scratchpad.softwarecounter=0;&#13;
&#13;
function raiseIncident(sDescription){&#13;
	var inc=new GlideRecord('incident');&#13;
	inc.initialize();&#13;
	inc.short_description="Issue with request no"+ current.number+" "+sDescription;&#13;
	inc.update();	&#13;
}&#13;
&#13;
function findAllAppGroups(sUserID){&#13;
	/*Returns all appgroups that a user belongs to*/	&#13;
	var oUser=new GlideRecord('sys_user');&#13;
	var asGroupList=[];&#13;
	if(!oUser.get(sUserID)){&#13;
		gs.addErrorMessage("No such user found");&#13;
		return [];&#13;
	}&#13;
	var oRelationshipq=new GlideRecord('sys_user_grmember');&#13;
	oRelationshipq.addQuery('user',oUser.sys_id);&#13;
	oRelationshipq.addQuery('group.name','STARTSWITH','App_');&#13;
	oRelationshipq.query();&#13;
	while(oRelationshipq.next()){&#13;
		//some Appgroup exists&#13;
		asGroupList.push(oRelationshipq.group.name);&#13;
	}&#13;
	oRelationshipq.deleteMultiple();//delete membership in SNOW&#13;
	return asGroupList;&#13;
}&#13;
&#13;
</value>
