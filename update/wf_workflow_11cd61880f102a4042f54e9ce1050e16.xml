        <value>function raiseIncident(sDescription){&#13;
	var inc=new GlideRecord('incident');&#13;
	inc.initialize();&#13;
	inc.short_description="Problem with offboarding user"+&#13;
		workflow.scratchpad.userName+" "+&#13;
		sDescription;&#13;
	inc.update();	&#13;
}&#13;
&#13;
var assets=new GlideRecord('alm_hardware');&#13;
//assets.addQuery('model',req_model);&#13;
assets.addQuery('assigned_to',workflow.scratchpad.userID); &#13;
assets.addOrCondition('reserved_for',workflow.scratchpad.userID);//In stock is 6&#13;
assets.query();&#13;
//assets.next();&#13;
if(assets.getRowCount()==0){&#13;
	gs.addInfoMessage("User does not have any hardware");&#13;
}&#13;
else{&#13;
	while(assets.next()){&#13;
		assets.install_status=6; //to be moved back to stock&#13;
		assets.update();&#13;
	}	&#13;
}</value>
