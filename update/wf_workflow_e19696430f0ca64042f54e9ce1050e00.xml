        <sys_updated_on>2016-06-21 13:19:42</sys_updated_on>
        <value>var sChennaiS1="ChennaiDemoUsers-S1";&#13;
var sChennaiS2="ChennaiDemoUsers-S2";&#13;
&#13;
var oGroupS1=new GlideRecord('sys_user_group');&#13;
var oGroupS2=new GlideRecord('sys_user_group');&#13;
var oUser=new GlideRecord('sys_user');&#13;
if(!oUser.get(current.requestor)){&#13;
	gs.addErrorMessage("Can't find a user who matches the requestor id: "+current.requestor);&#13;
}&#13;
&#13;
if(oGroupS1.get(sChennaiS1)){&#13;
	//group also exists		&#13;
	var qRelationship = new GlideRecord('sys_user_grmember');&#13;
	qRelationship.addQuery('user',current.requestor);&#13;
	qRelationship.addQuery('group',oGroupS1.sys_id);&#13;
	qRelationship.query();&#13;
	if(qRelationship.next()){&#13;
		//So user has made the request and they belong to S1&#13;
		if(oGroupS2.get(sChennaiS2)){&#13;
			/*We add user to group S2*/&#13;
			var oRelS2 = new GlideRecord('sys_user_grmember');&#13;
			oRelS2.initialize();&#13;
			oRelS2.user=current.requestor;&#13;
			oRelS2.group=oGroupS2.sys_id;&#13;
			oRelS2.insert();&#13;
		}&#13;
		else{&#13;
			gs.addErrorMessage("Group "+sChennaiS2+" does not exist!");&#13;
		}&#13;
	}&#13;
	else{&#13;
		gs.addErrorMessage("No user group relationship between user "+ &#13;
						   oUser.getDisplayValue()+&#13;
						   " and group :"+&#13;
						   sChennaiS1+&#13;
						   "exists!"&#13;
						  );&#13;
	}&#13;
	qRelationship.deleteMultiple();&#13;
}&#13;
else{&#13;
	gs.addErrorMessage("Group "+sChennaiS1+ " doesn't exist!");&#13;
}&#13;
//var sChennaiS3="ChennaiDemoUsers";&#13;
&#13;
</value>
